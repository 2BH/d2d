DESCRIPTION
"Model of the PI3K-EGFR signaling pathway"
"This model is taken from the paper "Decoupling of Receptor and Downstream Signals in the Akt Pathway by Its Low-Pass Filter Characteristics" , by Fujita et. al., in 2010, ScienceSignaling, Vol.3 Issue 132 ra56"
"https://www.ncbi.nlm.nih.gov/pubmed/20664065"

PREDICTOR
t	T	"min"	"time"	0	240

COMPARTMENTS
cyt	V	"pl"	"vol."	1

STATES
pro_EGFR	C	mumol/l	conc.	cyt	1	"pro_EGFR"
EGFR	C	mumol/l	conc.	cyt	1	"EGFR"
EGF_EGFR	C	mumol/l	conc.	cyt	1	"EGF_EGFR"
pEGFR	C	mumol/l	conc.	cyt	1	"pEGFR"
Akt	C	mumol/l	conc.	cyt	1	"Akt"
pEGFR_Akt	C	mumol/l	conc.	cyt	1	"pEGFR_Akt"
pAkt	C	mumol/l	conc.	cyt	1	"pAkt"
S6	C	mumol/l	conc.	cyt	1	"S6"
pAkt_S6	C	mumol/l	conc.	cyt	1	"pAkt_S6"
pS6	C	mumol/l	conc.	cyt	1	"pS6"

INPUTS
EGF	C	au	conc.	"EGF_conc_step"
EGF_conc_impulse	C	au	conc.	"EGF_conc_impulse"
EGF_conc_ramp	C	au	conc.	"EGF_conc_ramp"
pulse_time	C	au	conc.	"pulse_time"
ramp_time	C	au	conc.	"ramp_time"

ODES
"0"
"EGF_EGFR*re3_k2 - EGFR_turnover*(EGFR - pro_EGFR) - EGF*EGFR*re3_k1"
"EGF*EGFR*re3_k1 - EGF_EGFR*re4_k1 - EGF_EGFR*re3_k2"
"EGF_EGFR*re4_k1 - pEGFR*re5_k1 + pEGFR_Akt*re6_1_k2 + pEGFR_Akt*re6_2_k1 - Akt*pEGFR*re6_1_k1"
"pAkt*re7_k1 + pEGFR_Akt*re6_1_k2 - Akt*pEGFR*re6_1_k1"
"Akt*pEGFR*re6_1_k1 - pEGFR_Akt*re6_2_k1 - pEGFR_Akt*re6_1_k2"
"pAkt_S6*re8_1_k2 - pAkt*re7_k1 + pAkt_S6*re8_2_k1 + pEGFR_Akt*re6_2_k1 - S6*pAkt*re8_1_k1"
"pAkt_S6*re8_1_k2 + pS6*re9_k1 - S6*pAkt*re8_1_k1"
"S6*pAkt*re8_1_k1 - pAkt_S6*re8_2_k1 - pAkt_S6*re8_1_k2"
"pAkt_S6*re8_2_k1 - pS6*re9_k1"

DERIVED
pEGFR_tot_abs	C	mumol/l	conc.	"pEGFR_scaleFactor*(pEGFR + pEGFR_Akt)"
pAkt_tot_abs	C	mumol/l	conc.	"pAkt_scaleFactor*(pAkt + pAkt_S6)"
pS6_tot_abs	C	mumol/l	conc.	"pS6*pS6_scaleFactor"

OBSERVABLES

ERRORS

CONDITIONS
init_pro_EGFR	"6.8190e+04"
init_EGFR	"init_EGFR"
init_EGF_EGFR	"0"
init_pEGFR	"0"
init_Akt	"init_AKT"
init_pEGFR_Akt	"0"
init_pAkt	"0"
init_S6	"init_S6"
init_pAkt_S6	"0"
init_pS6	"0"
