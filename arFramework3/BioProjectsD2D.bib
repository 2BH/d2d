	Journal = {Chemical Communications},
	Keywords = {Animals, Bacterial Proteins, Biocatalysis, CHO Cells, Cricetinae, Cricetulus, Cyanobacteria, Heme Oxygenase-1, Light, Oxidoreductases Acting on CH-CH Group Donors, Phycobilins, Phycocyanin, Plasmids, RNA Interference, RNA, Small Interfering},
	Number = {79},
